{
  "name": "matter-fireworks",
  "version": "1.0.2",
  "description": "Displays fireworks using Matter.js",
  "main": "dist/Fireworks.js",
  "scripts": {
    "build": "babel src -d dist"
  },
  "author": "Avram Walden (aviemet)",
  "repository": "https://github.com/aviemet/matter-fireworks",
  "license": "ISC",
  "dependencies": {
    "matter-js": "^0.14.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
